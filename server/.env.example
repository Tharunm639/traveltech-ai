# Copy to .env and fill values
MONGO_URI=mongodb://127.0.0.1:27017/traveltech
PORT=5000
# Simple dev admin token for admin endpoints. Keep secret in production and use real auth.
DEV_ADMIN_TOKEN=changeme_dev_token
# JWT configuration
JWT_SECRET=replace_with_strong_secret
JWT_EXPIRES_IN=7d
## AI provider configuration
# Choose provider name (e.g. 'anthropic')
AI_PROVIDER=anthropic
# Model to use (e.g. 'claude-haiku-4.5')
AI_MODEL=claude-haiku-4.5
# Anthropic API key (do NOT commit real keys)
ANTHROPIC_API_KEY=
